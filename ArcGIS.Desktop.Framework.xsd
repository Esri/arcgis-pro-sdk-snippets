  Copyright (c) ESRI Corporation. All rights reserved.
-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.esri.com/DADF/Registry" 
           targetNamespace="http://schemas.esri.com/DADF/Registry" 
           elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<!-- ArcGIS Root Element -->
	<xs:element name="ArcGIS">
		<xs:annotation>
			<xs:documentation>ArcGIS root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
      <xs:all minOccurs="0" >
        <xs:element name="AddInInfo" type="CT_AddInInfo" minOccurs="0"/>
        <xs:element name="accelerators" type="CT_UpdateAccelerators" minOccurs="0"/>
        <xs:element name="DockPaneSets" type="CT_DockPaneSets" minOccurs="0"/>
        <xs:element name="products" type="CT_Products" minOccurs="0" maxOccurs="1"/>
        <xs:element name="conditions" type="CT_InsertConditions" minOccurs="0"/>
        <xs:element name="modules" type="CT_UpdateModules" minOccurs="0"/>
        <xs:element name="propertySheets" type="CT_UpdateSheets" minOccurs="0"/>
        <xs:element name="backstage" type="CT_Backstage" minOccurs="0"/>
        <xs:element name="categories" type="CT_ComponentCategories" minOccurs="0"/>
        <xs:element name="wizards" type="CT_Wizards" minOccurs="0"/>
        <xs:element name="dialogs" type="CT_Dialogs" minOccurs="0"/>
        <xs:element name="dependencies" type="CT_Dependencies" minOccurs="0"/>
        <xs:element name="ribbonToolbar" type="CT_RibbonToolbars" minOccurs="0"/>
        <xs:element name="quickAccessToolbar" type="CT_QAT" minOccurs="0"/>
        <xs:element name="dropHandlers" type="CT_UpdateDropHandlers" minOccurs="0"/>
        <xs:element name="shortcutTables" type="CT_InsertShortcutTables" minOccurs="0"/>
        <xs:element name="Configuration" type="CT_Configuration" minOccurs="0" maxOccurs="1"/>
      </xs:all>
			<xs:attribute name="defaultAssembly" type="ST_String" use="optional"/>
			<xs:attribute name="defaultNamespace" type="ST_String" use="optional"/>
			<xs:attribute name="defaultPublicKeyToken" type="ST_String" use="optional"/>
			<xs:attribute name="defaultVersion" type="ST_String" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CT_AddInInfo">
		<xs:sequence>
			<xs:element name="Name" minOccurs="1"/>
			<xs:element name="Description" minOccurs="0"/>
			<xs:element name="Image" minOccurs="0"/>
			<xs:element name="Author" minOccurs="0"/>
			<xs:element name="Company" minOccurs="0"/>
			<xs:element name="Date" minOccurs="0"/>
			<xs:element name="Subject" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string" use="required"/>
		<xs:attribute name="version" type="xs:string" use="required"/>
		<xs:attribute name="desktopVersion" type="xs:string" use="required"/>
	</xs:complexType>
	<!--
	 Dependencies 
	-->
	<xs:complexType name="CT_Dependencies">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="dependency" type="CT_FileDependency"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_FileDependency">
		<xs:attribute name="name" type="ST_String" use="required"/>
	</xs:complexType>
	<xs:complexType name="CT_Dependency">
		<xs:attribute name="refID" type="ST_ID" use="required"/>
	</xs:complexType>
  <!--
	 Products
	-->
  <xs:complexType name="CT_Products">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="insertProduct">
        <xs:complexType>
          <xs:attribute name="id" type="ST_ID" use="required"/>
          <xs:attribute name="caption" type="xs:string" use="required"/>
          <xs:attribute name="description" type="xs:string" use="required"/>
          <xs:attribute name="code" type="xs:string" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
	<!--
	 Backstage
	-->
	<xs:complexType name="CT_Backstage">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="insertButton" type="CT_BackstageButton"/>
			<xs:element name="insertTab" type="CT_BackstageTab"/>
			<xs:element name="updateTab" type="CT_BackstageTabUpdate"/>
			<xs:element name="deleteTab" type="CT_Delete"/>
			<xs:element name="deleteButton" type="CT_Delete"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_BackstageButton">
		<xs:attribute name="refID" type="ST_ID" use="required"/>
		<xs:attribute name="insert" type="ST_Insert" use="optional"/>
		<xs:attribute name="placeWith" type="ST_ID" use="optional"/>
		<xs:attribute name="separator" type="xs:boolean" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_BackstageTab">
		<xs:choice minOccurs="0" maxOccurs="1">
			<xs:element name="content" type="CT_Content"/>
		</xs:choice>
		<xs:attributeGroup ref="simpleComponentGroup"/>
		<xs:attribute name="keytip" type="ST_String" use="required"/>
		<xs:attribute name="condition" type="ST_ID" use="optional"/>
    <xs:attribute name="insert" type="ST_Insert" use="optional"/>
    <xs:attribute name="placeWith" type="ST_ID" use="optional"/>
    <xs:attribute name="separator" type="xs:boolean" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_BackstageTabUpdate">
		<xs:attribute name="refID" type="ST_ID" use="required"/>
		<xs:attribute name="caption" type="ST_String" use="optional"/>
	</xs:complexType>
	<!--
	 Conditions
	-->
	<xs:complexType name="CT_Conditions">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="condition" type="CT_Condition"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_InsertConditions">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="insertCondition" type="CT_Condition"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_Condition">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="state" type="CT_State"/>
			<xs:element name="and" type="CT_Operator"/>
			<xs:element name="or" type="CT_Operator"/>
			<xs:element name="not" type="CT_Operator"/>
		</xs:choice>
		<xs:attribute name="id" type="ST_UniqueID" use="required"/>
		<xs:attribute name="caption" type="ST_String" use="optional"/>
    <xs:attribute name="description" type="ST_String" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_State">
		<xs:attribute name="id" type="ST_ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="CT_Operator">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="state" type="CT_State"/>
			<xs:element name="and" type="CT_Operator"/>
			<xs:element name="or" type="CT_Operator"/>
			<xs:element name="not" type="CT_Operator"/>
		</xs:choice>
	</xs:complexType>

  <!--
	 ShortcutTables
	-->
  <xs:complexType name="CT_InsertShortcutTables">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="insertShortcutTable" type="CT_ShortcutTable"/>
      <xs:element name="updateShortcutTable" type="CT_UpdateShortcutTable"/>
      <xs:element name="deleteShortcutTable" type="CT_DeleteShortcutTable"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="CT_ShortcutTable">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="shortcut" type="CT_Shortcut"/>
    </xs:choice>
    <xs:attribute name="id" type="ST_UniqueID" use="required"/>
    <xs:attribute name="targetID" type="ST_ID" use="required"/>
    <xs:attribute name="category" type="xs:string" use="required"/>
    <xs:attribute name="description" type="xs:string" use="optional"/>
    <xs:attribute name="caption" type="xs:string" use="required"/>
    <!--<xs:attribute name="mode" type="xs:string" use="required"/>-->
    <xs:attribute name="condition" type="ST_ID" use="optional"/>
  </xs:complexType>
  <xs:complexType name="CT_UpdateShortcutTable">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="insertShortcut" type="CT_Shortcut"/>
      <xs:element name="deleteShortcut" type="CT_Delete"/>
    </xs:choice>
    <xs:attribute name="refID" type="ST_ID" use="required"/>
  </xs:complexType>
  <xs:complexType name="CT_DeleteShortcutTable">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="deleteShortcut" type="CT_Delete"/>
    </xs:choice>
    <xs:attribute name="refID" type="ST_ID" use="required"/>
  </xs:complexType>
  <xs:complexType name="CT_Shortcut">
    <xs:attribute name="refID" type="ST_ID" use="required"/>
    <xs:attribute name="key" type="ST_String" use="required"/>
    <xs:attribute name="flags" type="ST_ShortcutKeyFlag" use="optional"/>
    <xs:attribute name="onKeyUp" type="xs:boolean" use="optional"/>
    <xs:attribute name="cancelOnMouseMove" type="xs:boolean" use="optional"/>
  </xs:complexType>
  <xs:complexType name="CT_QAT">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="control" type="CT_Control"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="CT_Control">
    <xs:attribute name="refID" type="ST_ID" use="required"/>
    <xs:attribute name="size" type="ST_Size" use="optional"/>
    <xs:attribute name="isChecked" type="xs:boolean" use="optional"/>
  </xs:complexType>
	<!--
	 RibbonToolbar 
	-->
	<xs:complexType name="CT_RibbonToolbars">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="control" type="CT_RibbonToolbar"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_RibbonToolbar">
		<xs:complexContent>
			<xs:extension base="CT_Dependency">
				<xs:attribute name="size" type="ST_Size" use="optional"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

  <!--
	 SubGroups 
	-->
  <xs:complexType name="CT_Subgroups">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="subgroup" type="CT_Subgroup"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="CT_Subgroup">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="3">
        <xs:element name="button" type="CT_SizedToolbarButtonRef"/>
        <xs:element name="tool" type="CT_SizedToolbarButtonRef"/>
        <xs:element name="menu" type="CT_SizedMenuButtonRef"/>
        <xs:element name="splitButton" type="CT_SplitButtonRef"/>
        <xs:element name="editBox" type="CT_EditBoxRef"/>
        <xs:element name="comboBox" type="CT_ComboBoxRef"/>
        <xs:element name="labelControl" type="CT_LabelControlRef"/>
        <xs:element name="checkBox" type="CT_SizedButtonRef"/>
        <xs:element name="gallery" type="CT_SizedGalleryRef"/>
        <xs:element name="toolPalette" type="CT_PaletteRef"/>
        <xs:element name="buttonPalette" type="CT_PaletteRef"/>
        <xs:element name="spinner" type="CT_SizedButtonRef"/>
        <xs:element name="dynamicMenu" type="CT_SizedMenuButtonRef"/>
        <xs:element name="customControl" type="CT_SizedCustomControlRef"/>
      </xs:choice>
    </xs:sequence>
    <xs:attribute name="id" type="ST_UniqueID" use="required"/>
    <xs:attribute name="size" use="optional" type="ST_SubGroupSize"/>
    <xs:attribute name="horizontalAlignment" use="optional" type="ST_HorizontalAlignment"/>
    <xs:attribute name="verticalAlignment" use="optional" type="ST_VerticalAlignment"/>
  </xs:complexType>
  <xs:complexType name="CT_SubgroupRef">
    <xs:attribute name="refID" type="ST_ID" use="required"/>
  </xs:complexType>
  <xs:complexType name="CT_UpdateSubgroups">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="deleteSubgroup" type="CT_Delete"/>
      <xs:element name="insertSubgroup" type="CT_Group"/>
      <xs:element name="updateSubgroup" type="CT_UpdateSubgroup"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="CT_UpdateSubgroup">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="deleteButton" type="CT_Delete"/>
      <xs:element name="insertButton" type="CT_SizedToolbarButtonRef"/>
      <xs:element name="deleteTool" type="CT_Delete"/>
      <xs:element name="insertTool" type="CT_SizedToolbarButtonRef"/>
      <xs:element name="deleteMenu" type="CT_Delete"/>
      <xs:element name="insertMenu" type="CT_SizedMenuButtonRef"/>
      <xs:element name="deleteEditBox" type="CT_Delete"/>
      <xs:element name="insertEditBox" type="CT_EditBoxRef"/>
      <xs:element name="deleteComboBox" type="CT_Delete"/>
      <xs:element name="insertComboBox" type="CT_ComboBoxRef"/>
      <xs:element name="deleteLabelControl" type="CT_Delete"/>
      <xs:element name="insertLabelControl" type="CT_LabelControlRef"/>
      <xs:element name="deleteCheckBox" type="CT_Delete"/>
      <xs:element name="insertCheckBox" type="CT_Dependency"/>
      <xs:element name="deleteGallery" type="CT_Delete"/>
      <xs:element name="insertGallery" type="CT_SizedGalleryRef"/>
      <xs:element name="deleteSplitButton" type="CT_Delete"/>
      <xs:element name="insertSplitButton" type="CT_SplitButtonRef"/>
      <xs:element name="deleteToolPalette" type="CT_Delete"/>
      <xs:element name="insertToolPalette" type="CT_PaletteRef"/>
      <xs:element name="deleteButtonPalette" type="CT_Delete"/>
      <xs:element name="insertButtonPalette" type="CT_PaletteRef"/>
      <xs:element name="updateButtonPalette" type="CT_PaletteRef"/>
      <xs:element name="insertSpinner" type="CT_Spinner"/>
      <xs:element name="deleteSpinner" type="CT_Delete"/>
      <xs:element name="deleteDynamicMenu" type="CT_Delete"/>
      <xs:element name="insertDynamicMenu" type="CT_SizedMenuButtonRef"/>
      <xs:element name="insertCustomControl" type="CT_SizedButtonRef"/>
      <xs:element name="deleteCustomControl" type="CT_Delete"/>
    </xs:choice>
    <xs:attributeGroup ref="AG_UpdateDeclarativeGroup"/>
  </xs:complexType>
  <!--
    Configuration
  -->
  <xs:complexType name="CT_Configuration">
    <xs:sequence>
      <xs:element name="ConfigurationManager" type="CT_Content" minOccurs="0" maxOccurs="1"/>
      <xs:element name="AddIns" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:choice>
            <xs:element name="AdditionalWellKnownFolder" minOccurs="0" maxOccurs="unbounded"/>
          </xs:choice>
          <xs:attribute name="securityLevel"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="blockCustomizeDialog" use="optional" type="xs:boolean" default="false"/>
    <xs:attribute name="blockPerfMeter" use="optional" type="xs:boolean" default="false"/>
    <xs:attribute name="checkForUpdatesAtStartup" use="optional" type="xs:boolean" default="true"/>
    <xs:attribute name="blockAddInTab" use="optional" type="xs:boolean" default="false"/>
  </xs:complexType>
  
	<!--
	 Dialogs 
	-->
	<xs:complexType name="CT_Dialogs">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="dialog" type="CT_Dialog"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_Dialog">
		<xs:choice minOccurs="0" maxOccurs="1">
			<xs:element name="content" type="CT_Content"/>
		</xs:choice>
		<xs:attribute name="id" type="ST_UniqueID" use="required"/>
		<xs:attribute name="className" use="required"/>
		<xs:attribute name="assembly" use="optional"/>
		<xs:attribute name="publicKeyToken" use="optional"/>
		<xs:attribute name="version" use="optional"/>
	</xs:complexType>
	<!--
	 Wizards 
	-->
	<xs:complexType name="CT_Wizards">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="insertWizard" type="CT_Wizard"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_Wizard">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="page" type="CT_Page"/>
		</xs:choice>
		<xs:attribute name="id" type="ST_UniqueID" use="required"/>
		<xs:attribute name="pageHeight" type="xs:integer" use="required"/>
		<xs:attribute name="pageWidth" type="xs:integer" use="required"/>
		<xs:attribute name="isPageListVisible" type="xs:boolean"/>
    <xs:attribute name="canMoveNextOnValidOnly" type="xs:boolean"/>
    <xs:attribute name="isPageCountVisible" type="xs:boolean"/>
		<xs:attribute name="resizable" type="xs:boolean"/>
		<xs:attribute name="caption" type="ST_String" use="required"/>
		<xs:attribute name="showTitleBar" type="xs:boolean"/>
	</xs:complexType>
	<!--
	 Property Sheets 
	-->
	<xs:complexType name="CT_PropertySheets">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="sheet" type="CT_Sheet"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_UpdateSheets">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="deleteSheet" type="CT_Delete"/>
			<xs:element name="insertSheet" type="CT_Sheet"/>
			<xs:element name="updateSheet" type="CT_UpdateSheet"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_Sheet">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="page" type="CT_Page"/>
			<xs:element name="category" type="CT_Category"/>
		</xs:choice>
		<xs:attribute name="id" type="ST_UniqueID" use="required"/>
    <xs:attribute name="instanceID" type="ST_ID" use="optional"/>
		<xs:attribute name="caption" type="ST_String" use="required"/>
		<xs:attribute name="pageWidth" type="xs:integer" use="optional"/>
		<xs:attribute name="pageHeight" type="xs:integer" use="optional"/>
		<xs:attribute name="hideApply" type="xs:boolean" use="optional"/>
		<xs:attribute name="resizable" type="xs:boolean" use="optional"/>
		<xs:attribute name="pageListWidth" type="xs:string" use="optional"/>
		<xs:attribute name="hasGroups" type="xs:boolean" use="optional"/>
    <xs:attribute name="isSearchEnabled" type="xs:boolean" use="optional"/>
    <xs:attribute name="allowPagesWithErrors" type="xs:boolean" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_UpdateSheet">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="deletePage" type="CT_Delete"/>
			<xs:element name="insertPage" type="CT_Page"/>
			<xs:element name="updatePage" type="CT_UpdatePage"/>
			<xs:element name="insertCategory" type="CT_Category"/>
			<xs:element name="updateCategory" type="CT_UpdateCategory"/>
			<xs:element name="deleteCategory" type="CT_Delete"/>
		</xs:choice>
		<xs:attribute name="refID" type="ST_ID" use="required"/>
		<xs:attribute name="caption" type="ST_String" use="optional"/>
		<xs:attribute name="pageWidth" type="xs:integer" use="optional"/>
		<xs:attribute name="pageHeight" type="xs:integer" use="optional"/>
		<xs:attribute name="hideApply" type="xs:boolean" use="optional"/>
		<xs:attribute name="resizable" type="xs:boolean" use="optional"/>
	</xs:complexType>
	<!--
	 Property Pages
	-->
	<xs:complexType name="CT_Page">
		<xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="1">
				<xs:element name="tooltip" type="CT_Tooltip" minOccurs="0"/>
        <xs:element name="searchTerms" minOccurs="0"/>
			</xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="1">
				<xs:element name="content" type="CT_Content"/>
			</xs:choice>
		</xs:sequence>
		<xs:attributeGroup ref="simpleComponentGroup"/>
		<xs:attribute name="group" type="ST_String" use="optional"/>
		<xs:attribute name="condition" type="ST_ID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_Category">
		<xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="1">
				<xs:element name="tooltip" type="CT_Tooltip"/>
			</xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="category" type="CT_Category"/>
				<xs:element name="page" type="CT_Page"/>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="id" type="ST_ID" use="required"/>
		<xs:attribute name="caption" type="ST_String" use="required"/>
		<xs:attribute name="image" type="ST_String" use="optional"/>
		<xs:attribute name="condition" type="ST_ID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_UpdatePage">
		<xs:sequence minOccurs="0" maxOccurs="1">
			<xs:element name="tooltip" type="CT_Tooltip"/>
		</xs:sequence>
		<xs:attributeGroup ref="AG_UpdateDeclarativeGroup"/>
		<xs:attribute name="insert" type="ST_Insert" use="optional"/>
		<xs:attribute name="placeWith" type="ST_ID" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CT_UpdateCategory">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="deletePage" type="CT_Delete"/>
			<xs:element name="insertPage" type="CT_Page"/>
			<xs:element name="updatePage" type="CT_UpdatePage"/>
			<xs:element name="insertCategory" type="CT_Category"/>
			<xs:element name="updateCategory" type="CT_UpdateCategory"/>
			<xs:element name="deleteCategory" type="CT_Delete"/>
		</xs:choice>
		<xs:attribute name="refID" type="ST_ID" use="required"/>
		<xs:attribute name="caption" type="ST_String" use="optional"/>
	</xs:complexType>
	<!--
    Categories and Components
  -->
	<xs:complexType name="CT_ComponentCategories">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="deleteCategory" type="CT_Delete"/>
			<xs:element name="insertCategory" type="CT_ComponentCategory"/>
			<xs:element name="updateCategory" type="CT_UpdateComponentCategory"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_ComponentCategory">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="component" type="CT_Component"/>
		</xs:choice>
		<xs:attribute name="id" type="ST_UniqueID" use="required"/>
	</xs:complexType>
	<xs:complexType name="CT_UpdateComponentCategory">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="insertComponent" type="CT_Component"/>
			<xs:element name="deleteComponent" type="CT_Delete"/>
		</xs:choice>
		<xs:attribute name="refID" type="ST_ID" use="required"/>
	</xs:complexType>
	<xs:complexType name="CT_Component">
    <xs:sequence minOccurs="0">
      <xs:element name="content">
        <xs:complexType>
          <xs:sequence>
            <xs:any namespace="##any" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:anyAttribute processContents="skip"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
	  <xs:attribute name="id" type="ST_UniqueID" use="required"/>
    <xs:attribute name="className" type="xs:string" use="optional"/>
    <xs:anyAttribute namespace="##any" processContents="skip"/>
	</xs:complexType>
	<!--
	 Modules
	-->
	<xs:complexType name="CT_Modules">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="module" type="CT_Module"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_UpdateModules">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="insertModule" type="CT_Module"/>
			<xs:element name="updateModule" type="CT_UpdateModule"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="CT_Module">
